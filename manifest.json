{
  "manifest_version": 3,
  "name": "GitHub Full Time Display",
  "version": "1.1",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "description": "Display full run time on GitHub without hover."
}
